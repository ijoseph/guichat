package client;

/**
 * Class to represent a past message, to be handled by the GUI
 * @author Jesse
 *
 */

public class PastMessage {
	public final String contents;
	public final String source;
	
	public PastMessage(String contents, String source) {
		this.contents = contents;
		this.source = source;
	}
	
	@Override
	public String toString() {
		return "Message from " + source + ": " + contents;
	}
}
